<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="awesome_owl.counter">
      <Center>
          <div>
          <p> Counter : <t t-esc="state.value"/></p>
          </div>
            <group name="actions" string="Actions">
                <button class="btn btn-xs btn-primary" t-on-click="increment"><i class="fa fa-plus"></i></button>
                <button class="btn btn-xs btn-secondary" t-on-click="decrement"><i class="fa fa-minus"></i></button>
           </group>
      </Center>
    </t>

</templates>
